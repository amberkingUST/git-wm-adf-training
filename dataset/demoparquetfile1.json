{
	"name": "demoparquetfile1",
	"properties": {
		"linkedServiceName": {
			"referenceName": "ADLSConnection",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"dlSchema": {
				"type": "String"
			},
			"dlTable": {
				"type": "String"
			},
			"dateTime": {
				"type": "String"
			}
		},
		"annotations": [],
		"type": "Parquet",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@dataset().dateTime",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@concat(dataset().dlSchema,'/',dataset().dlTable)",
					"type": "Expression"
				},
				"fileSystem": "akdemodata"
			},
			"compressionCodec": "gzip"
		},
		"schema": []
	}
}