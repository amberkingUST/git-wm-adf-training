{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-wm-adf-training-dev"
		},
		"ADLSConnection_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLSConnection'"
		},
		"ADLSConnection_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlswmadftraining.dfs.core.windows.net"
		},
		"kvwmadftraining_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-wm-adf-training.vault.azure.net/"
		},
		"sqlconnection_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "@linkedService().Environment"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-wm-adf-training-dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLSConnection_accountKey": {
						"value": "[parameters('ADLSConnection_accountKey')]"
					},
					"ADLSConnection_properties_typeProperties_url": {
						"value": "[parameters('ADLSConnection_properties_typeProperties_url')]"
					},
					"kvwmadftraining_properties_typeProperties_baseUrl": {
						"value": "[parameters('kvwmadftraining_properties_typeProperties_baseUrl')]"
					},
					"sqlconnection_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('sqlconnection_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}